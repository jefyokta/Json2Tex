{
    "name": "jefyokta/json2tex",
    "description": "tiptap or prosess mirror json converter to html & tex",
    "license": "mit",
    "keywords": ["tiptap", "json", "latex", "converter"],
    "type":"library",
    "scripts":{
        "test":["./vendor/bin/pest"]
    },
    "autoload": {
        "psr-4": {
            "Jefyokta\\Json2Tex\\": "src/",
            "Tests\\":"tests/"
        }
    },
    "authors": [
        {
            "name": "Jepi Oktamipa",
            "email": "jefyokta50@gmail.com"
        }
    ],
    "require": {
        "php":">=7.2"
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
